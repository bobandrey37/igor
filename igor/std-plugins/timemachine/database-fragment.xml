<?xml version="1.0" ?>
<data  xmlns:own="http://jackjansen.nl/igor/owner">
	<services>
		<backup.desktop own:plugin="{plugin}"/>
	</services>
	<actions>
		<action own:plugin="{plugin}">
			<interval>36000</interval>
			<url>/pluginscript/{plugin}/get?name=backup.desktop&amp;remoteHost=localhost</url>
			<aggregate>true</aggregate>
		</action>
	</actions>
	<plugindata>
		<{plugin} own:plugin="{plugin}">
		</{plugin}>
	</plugindata>
</data>
