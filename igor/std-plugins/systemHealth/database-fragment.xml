<?xml version="1.0" ?>
<data xmlns:own="http://jackjansen.nl/igor/owner">
	<services>
		<router/>
		<internet/>
	</services>
	<actions>
		<action own:plugin="{plugin}">
			<name>systemHealth</name>
			<interval>60</interval>
			<xpath>/data/status/*/*/errorMessage</xpath>
			<url>/plugin/{plugin}</url>
		</action>
	</actions>
	<plugindata>
		<{plugin} own:plugin="{plugin}">
			<sensorMaxInterval/>
		</{plugin}>
	</plugindata>
</data>
