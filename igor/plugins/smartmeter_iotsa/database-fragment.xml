<?xml version="1.0" ?>
<data>
	<actions>
		<action>
			<interval>60</interval>
			<url>/pluginscripts/smartmeter_iotsa/pull</url>
			<representing>sensors/smartmeter_iotsa</representing>
			<aggregate>true</aggregate>
		</action>
		<action>
			<xpath>/data/sensors/smartMeter</xpath>
			<method>PUT</method>
			<url>/data/environment/energy/electricity</url>
			<mimetype>text/plain</mimetype>
			<data>{current_kw}</data>
		</action>
	</actions>
	<plugindata>
		<smartmeter_iotsa>
			<host>smartmeter.local</host>
			<protocol>https</protocol>
		</smartmeter_iotsa>
	</plugindata>
	<environment>
		<energy>
		</energy>
	</environment>
</data>
